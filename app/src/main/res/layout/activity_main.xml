        android:layout_alignParentTop="true"
        android:layout_marginLeft="67dp"
        android:layout_marginStart="67dp"
        android:layout_marginTop="266dp" />

    <RadioButton
        android:id="@+id/radioButton2"
        android:layout_width="wrap_content"
